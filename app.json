{
  "scheme": "com.hxzdqx.emsapp",
  "expo": {
    "name": "ems-app",
    "slug": "ems-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "experiments": {
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hxzdqx.emsapp"
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router", "expo-localization"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6afc095e-e0be-4a10-b818-4d3df82ace8b"
      }
    }
  }
}
